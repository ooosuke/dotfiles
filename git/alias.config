[alias]
  st = status
  noadd = reset HEAD
  cm = commit -m
  co = checkout
  br = branch
  df = diff
  rename = branch -m
  clear = reset --hard HEAD
  del = branch -D
  lg1 = log --oneline
  lg = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'